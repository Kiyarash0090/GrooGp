# JWT Secret Key - MUST be changed in production!
# Generate a strong random key: node -e "console.log(require('crypto').randomBytes(64).toString('hex'))"
JWT_SECRET= 

# Debug logging (set to true for development)
DEBUG_LOG=false

# Server Port (optional, defaults to 3000)
PORT=3000

# Google OAuth client ID used by the frontend
GOOGLE_CLIENT_ID=

# Admin Gmail (used for admin checks)
ADMIN_EMAIL=
